{
  "dashboard": {
    "id": null,
    "uid": "patient-vitals-dashboard",
    "title": "Patient Vitals Monitoring",
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "10s",
    "panels": [
      {
        "type": "timeseries",
        "title": "Heart Rate (BPM)",
        "datasource": "your-data-source",
        "targets": [
          {
            "refId": "A",
            "expr": "heart_rate"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "beatsperminute",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 12,
          "h": 8
        }
      },
      {
        "type": "timeseries",
        "title": "Oxygen Level (%)",
        "datasource": "your-data-source",
        "targets": [
          {
            "refId": "B",
            "expr": "oxygen_level"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "x": 12,
          "y": 0,
          "w": 12,
          "h": 8
        }
      },
      {
        "type": "timeseries",
        "title": "Blood Pressure",
        "datasource": "your-data-source",
        "targets": [
          {
            "refId": "C",
            "expr": "blood_pressure"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "pressuremmhg",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "x": 0,
          "y": 8,
          "w": 24,
          "h": 8
        }
      }
    ]
  },
  "overwrite": true
}
